import java.util.Scanner;

public class Best_Fit {
	
	public static void Bestfit(int trackNum,int[] parcels) {
		
		int n = num / (parcels[0]);
		
		
	}

	public static void main(String args[]) {
		
		Scanner sc = new Scanner(System.in);
		
